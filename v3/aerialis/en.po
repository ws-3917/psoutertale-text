msgstr "<16>{#k/0/0}* ...super duper smart."

msgid "<16>{#k/2/4}* Like..."
msgstr "<16>{#k/2/4}* Like..."

msgid "<16>* UNNATURALLY smart."
msgstr "<16>* UNNATURALLY smart."

