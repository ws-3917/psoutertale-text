msgstr "<25>{#f/0}* \"And the monster found them someone they might like.\""

msgid "<25>{#f/0}* \"And the monster was happy.\""
msgstr "<25>{#f/0}* \"And the monster was happy.\""

msgid "<25>{#f/5}* \"But the human stayed away for a long time.\""
