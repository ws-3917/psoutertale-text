msgid "<18>{#p/papyrus}{#f/4}... I DON'T EVEN WANT TO KNOW."
msgstr ""

msgid "<18>{#p/papyrus}{#f/4}NEXT TIME, I'M HAVING SANS READ A HAPPY STORY."
msgstr ""

msgid "<18>{#f/6}WITH AN -ACTUAL- HAPPY ENDING!"
msgstr ""

msgid "<18>{#f/5}WHERE EVERYBODY COMES AWAY SATISFIED!!"
msgstr ""

msgid "<18>{#f/7}AND WHERE NOBODY HAS TO DIE OR SAY GOODBYE!!!"
msgstr ""

msgid ""
"<18>{#p/papyrus}{#f/6}SHH...\n"
"(NO PHONE CALLS IN THE LIBRARBY!)"
