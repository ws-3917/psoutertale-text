msgid "<18>{#p/papyrus}{#f/4}WELL... THAT ROOM'S VERY HOT."
msgstr ""

msgid "<18>{#p/papyrus}{#f/4}ON FIRE, IN FACT."
msgstr ""

msgid ""
"{#p/undyne}{#f/17}* Good!!\n"
"* I like hot things!"
msgstr ""

msgid ""
"<18>{#p/papyrus}{#f/6}HOLD UP!\n"
"I'M STILL DRAWING!"
msgstr ""

msgid "<18>{#p/papyrus}{#f/1}DO -I- KNOW WHO PAPYRUS IS!?!?"
msgstr ""

msgid "<18>{#p/papyrus}{#f/6}HEY, UH, WHILE YOU'RE THERE..."
msgstr ""

msgid "<18>{#f/6}WOULD YOU MIND LOOKING AFTER MY ACTION FIGURES??"
msgstr ""

msgid "<18>{#f/5}THEY'VE BEEN... FEELING KIND OF LONELY LATELY."
msgstr ""

msgid "<18>{#f/5}... THANKS."
msgstr ""

