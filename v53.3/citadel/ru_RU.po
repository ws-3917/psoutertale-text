msgid ""
msgstr ""

msgid "<32>{#p/human}* (You feel a darkness gathering inside of you.)"
msgstr "<32>{#p/human}* (Вы чувствуете, как в вашем теле собирается тьма.)"

msgid "<32>{#p/human}* (You wish it were all just a bad dream.)"
msgstr "<32>{#p/human}* (Вы хотите, чтобы все это было просто кошмаром.)"

msgid "<32>{#p/human}* (You try your best to fight it, but nothing happens.)"
msgstr ""
"<32>{#p/human}* (Вы стараетесь бороться с этим, но ничего не происходит.)"

msgid "<32>{#p/human}* (You wish desperately that you could break free.)"
msgstr "<32>{#p/human}* (Вы отчаянно хотите освободиться от оков.)"

msgid "<32>{#p/human}* (You call out for help, but nobody comes.)"
msgstr "<32>{#p/human}* (Ты зовешь на помощь, но никто не приходит.)"

msgid "<32>{#p/human}* (You wish you could make this all just go away.)"
msgstr "<32>{#p/human}* (Вы хотите, чтобы все это исчезло.)"

msgid "<32>{#p/human}* (...)"
msgstr "<32>{#p/human}* (...)"

msgid ""
"<32>{#p/human}* (You take a deep breath, and ready yourself for the end.)"
msgstr "<32>AR #p/human* (Вы глубоко вдыхаете и готовьтесь к окончанию.)"

msgid "<32>{#p/human}* (You know what must be done.)"
msgstr "<32>{#p/human}* (Вы знаете, что нужно делать)."

msgid "REBOOT IN $(x)"
msgstr "Возврат в долларах США (x)"

msgid "{#p/human}(You take a deep breath.)"
msgstr "{#p/human} (Ты глубоко дышишь.)"

msgid "(You're filled with determination.)"
msgstr "(Вы полны решимости.)"

msgid "{#p/human}{#v/1}{@fill=#42fcff}It'll be alright in the end..."
msgstr "{#p/human}{#v/1}{@ fill = #42fcff} В конце концов все будет хорошо."

msgid "{@fill=#42fcff}Just take it steady..."
msgstr "{@ fill = #42fcff} Стабильно."

msgid "{#p/human}{#v/2}{@fill=#ff993d}Now is no time to give up."
msgstr "{#p/human}{#v/2}{@ fill = #ff993d} Сейчас не время сдаваться."

msgid "{@fill=#ff993d}Get back out there!"
msgstr "{@ fill = #ff993d} Вернись!"

msgid "{#p/human}{#v/3}{@fill=#003cff}You know what you're capable of."
msgstr "{#p/human}{#v/3}{@ fill = #003cff} Вы знаете, что можете сделать."

msgid "{@fill=#003cff}Don't hold back!"
msgstr "{@ fill = #003cff} Не отступайте!"

msgid "{#p/human}{#v/4}{@fill=#d535d9}You know you can survive this..."
msgstr ""
"{#p/human}{#v/4}{@ fill = #d535d9} Ты знаешь, что можешь пережить это."

msgid "{@fill=#d535d9}Keep moving forward."
msgstr "{@ fill = #d535d9} Продолжайте."

msgid "{#p/human}{#v/5}{@fill=#00c000}You're this world's last hope..."
msgstr "{#p/human}{#v/5}{@ fill = #00c000} Ты последняя надежда этого мира."

msgid "{@fill=#00c000}Believe in yourself!"
msgstr "{@ fill = #00c000} Верьте в себя!"

msgid "{#p/human}{#v/6}{@fill=#faff29}It's only a matter of time."
msgstr "{#p/human}{#v/6}{@ fill = #faff29} Это только вопрос времени."

msgid "<99>{*}{#p/human}{#v/6}{@fill=#faff29}A lost SOUL calls out to you."
msgstr ""
"<99>{*}{#p/human}{#v/6}{@ fill = #faff29} К тебе взывает потерянная душа."

msgid ""
"<99>{*}{@fill=#faff29}With {@mystify=PATIENCE}PATIENCE{@mystify=}, they may "
"escape."
msgstr ""
"<99>{*}{@ fill = #faff29} С терпением {@ mystery = PATIENCE}{@ mysteriy =} "
"они могут сбежать."

msgid ""
"<99>{*}{#p/human}{#v/1}{@fill=#42fcff}You must reach my Little Dipper..."
msgstr ""
"<99>{*}{#p/human}{#v/1}{@ fill = #42fcff} Вы должны найти мой Бэйдоу Семь "
"звезд."

msgid "<99>{*}{#p/human}(Press [Z] to teleport.)"
msgstr "<99>{*}{#p/human} (передача осуществляется по [Z]."

msgid "<99>{*}{#p/human}{#v/1}{@fill=#42fcff}The entity lies in wait."
msgstr ""
"<99>{*}{#p/human}{#v/1}{@ fill = #42fcff} Объект находится в состоянии "
"ожидания."

msgid "<99>{*}{@fill=#42fcff}With PATIENCE, you may survive it..."
msgstr "<99>{*}{@ fill = #42fcff} Имея терпение, вы можете выжить."

msgid ""
"<99>{*}{@fill=#faff29}With {@mystify=BRAVERY}BRAVERY{@mystify=}, they may "
"escape."
msgstr ""
"<99>{*}{@ fill = #faff29} С {@ mystery = BRAVERY} BRAVERY {@ mysteriy =} они"
" могут сбежать."

msgid ""
"<99>{*}{#p/human}{#v/2}{@fill=#ff993d}Don't leave without my Power Glove!"
msgstr ""
"<99>{*}{#p/human}{#v/2}{@ fill = #ff993d} Не бросайте мои сильные перчатки!"

msgid "<99>{*}{#p/human}(Press [Z] to explode.)"
msgstr "<99>{*}{#p/human} (взрыв нажатием [Z])."

msgid "<99>{*}{#p/human}{#v/2}{@fill=#ff993d}The entity looms above..."
msgstr ""
"<99>{*}{#p/human}{#v/2}{@ fill = #ff993d} Материал неявно появляется выше."

msgid "<99>{*}{@fill=#ff993d}With BRAVERY, you may overcome it!"
msgstr "<99>{*}{@ fill = #ff993d} С храбростью вы можете преодолеть ее!"

msgid ""
"<99>{*}{@fill=#faff29}With {@mystify=INTEGRITY}INTEGRITY{@mystify=}, they "
"can escape."
msgstr ""
"<99>{*}{@ fill = #faff29} При наличии {@ mysterity = INTEGRITY} честности {@"
" mysherity =} они могут сбежать."

msgid "<99>{*}{#p/human}{#v/3}{@fill=#003cff}I'll need my trusty Hoverboots."
msgstr ""
"<99>{*}{#p/human}{#v/3}{@ fill = #003cff} Мне нужны мои надежные сапоги на "
"воздушной подушке."

msgid "<99>{*}{#p/human}{#v/3}{@fill=#003cff}The entity holds its position."
msgstr ""
"<99>{*}{#p/human}{#v/3}{@ fill = #003cff} Объект сохраняет свою позицию."

msgid "<99>{*}{@fill=#003cff}With INTEGRITY, you can surpass it."
msgstr "<99>{*}{@ fill = #003cff} С честностью вы можете превзойти ее."

msgid ""
"<99>{*}{@fill=#faff29}With {@mystify=PERSEVERANCE}PERSEVERANCE{@mystify=}, "
"they can escape."
msgstr ""
"<99>{*}{@ fill = #faff29} С помощью PERSEVERANCE {@ mystery = PERSEVERANCE} "
"они могут сбежать."

msgid ""
"<99>{*}{#p/human}{#v/4}{@fill=#d535d9}A single Datapad can go a long way."
msgstr ""
"<99>{*}{#p/human}{#v/4}{@ fill = #d535d9} Один Datapad может пройти долгий "
"путь."

msgid "<99>{*}{#p/human}{#v/4}{@fill=#d535d9}The entity is losing its hold."
msgstr "<99>{*}{#p/human}{#v/4}{@ fill = #d535d9} Объекты теряют контроль."

msgid "<99>{*}{@fill=#d535d9}With PERSEVERANCE, you can outlast it!"
msgstr "<99>{*}{@ fill = #d535d9} С настойчивостью вы можете жить дольше!"

msgid ""
"<99>{*}{@fill=#faff29}With {@mystify=KINDNESS}KINDNESS{@mystify=}, they can "
"escape."
msgstr ""
"<99>{*}{@ fill = #faff29} При наличии {@ mystery = KINDNESS} доброты {@ "
"mysteriy =} они могут сбежать."

msgid "<99>{*}{#p/human}{#v/5}{@fill=#00c000}The Tablaphone will set me free!"
msgstr ""
"<99>{*}{#p/human}{#v/5}{@ fill = #00c000} Tablaphone сделает меня свободным!"

msgid "<99>{*}{#p/human}{#v/5}{@fill=#00c000}The entity is destabilizing..."
msgstr ""
"<99>{*}{#p/human}{#v/5}{@ fill = #00c000} Структура дестабилизируется."

msgid "<99>{*}{@fill=#00c000}With KINDNESS, you can outshine it..."
msgstr "<99>{*}{@ fill = #00c000} С добротой вы можете превзойти ее."

msgid ""
"<99>{*}{#p/human}{#v/6}{@fill=#faff29}The lost SOULs called out to you."
msgstr ""
"<99>{*}{#p/human}{#v/6}{@ fill = #faff29} Потерянные души кричат на вас."

msgid "<99>{*}{@fill=#faff29}With JUSTICE, you have answered."
msgstr "<99>{*}{@ fill = #faff29} Вы ответили справедливо."

msgid "<99>{*}{@fill=#faff29}You have freed them of their prisons."
msgstr "<99>{*}{@ fill = #faff29} Вы освободили их из тюрьмы."

msgid "<99>{*}{#p/human}{#v/1}{@fill=#42fcff}At last."
msgstr "<99>{*}{#p/human}{#v/1}{@ fill = #42fcff} Наконец - то."

msgid "<99>{*}{#p/human}{#v/2}{@fill=#ff993d}You're a hero!"
msgstr "<99>{*}{#p/human}{#v/2}{@ fill = #ff993d} Ты герой!"

msgid "<99>{*}{#p/human}{#v/3}{@fill=#003cff}You've done the right thing."
msgstr "<99>{*}{#p/human}{#v/3}{@ fill = #003cff} Вы поступили правильно."

msgid "<99>{*}{#p/human}{#v/4}{@fill=#d535d9}Thank you..."
msgstr "<99>{*}{#p/human}{#v/4}{@ fill = #d535d9} Спасибо."

msgid "<99>{*}{#p/human}{#v/5}{@fill=#00c000}You're the best...!"
msgstr "<99>{*}{#p/human}{#v/5}{@ fill = #00c000} Ты лучший."

msgid "<99>{*}{#p/human}{#v/6}{@fill=#faff29}Our power is yours now."
msgstr ""
"<99>{*}{#p/human}{#v/6}{@ fill = #faff29} Наша сила теперь принадлежит вам."

msgid "<99>{*}{@fill=#faff29}With it, the entity will collapse."
msgstr "<99>{*}{@ fill = #faff29} С ним сущность рухнет."

msgid "<99>{*}{@fill=#faff29}Then..."
msgstr "<99>{*}{@ fill = #faff29} Затем..."

msgid "<99>{*}{@fill=#faff29}... you will do what you must."
msgstr "<99>{*}{@ fill = #faff29}. Ты сделаешь то, что должен."

msgid "<99>{*}{@fill=#faff29}Now, end this!"
msgstr "<99>{*}{@ fill = #faff29} Теперь, заканчивайте все это!"

msgid "<99>{*}{#p/human}(Press [Z] to shoot.)"
msgstr "<99>{*}{#p/human} (нажмите [Z] для съемки)."

msgid "REBOOTING..."
msgstr "Начинается перезагрузка."

msgid "READY"
msgstr "Готово."

msgid "WARNING..."
msgstr "предупреждение..."

