msgstr "<32>{#p/human}* (Вы чувствуете, что монстры поблизости уменьшаются.)"

msgid "<32>{#p/human}* (But there was nothing left for you to do here.)"
msgstr "<32>{#p/human}* (Но здесь больше нечего делать)."

msgid "<32>{#p/basic}{#npc/a}* The grass is fading faster than I had thought."
msgstr "<32>{#p/basic}{#npc/a}* Трава увядает быстрее, чем я думал."

msgid "<32>{#p/basic}* It's out of service."
msgstr "<32>{#p/basic}* Он прекратил обслуживание."

msgid "<32>{#p/human}* (The terminal appears to have been powered off.)"
msgstr "<32>{#p/human}* (Терминал, как представляется, отключен.)"

msgid "<32>{#p/human}* (You activate the terminal.)"
msgstr "<32>{#p/human}* (Вы активируете терминал)."

msgid ""
"<32>{#p/basic}* \"Pathway unlocked!\"\n"
"* \"No further action required.\""
msgstr ""
"<32>{#p/basic}* \"Путь разблокирован!\"\n"
"* \"Дальнейшие действия не требуются.\""

