msgstr "<32>{#p/human}* (Вы пытаетесь укусить сэндвич с привидениями)."

msgid "<32>{#p/human}* (You phase right through it.)"
msgstr "<32>{#p/human}* (Вы можете сделать это поэтапно.)"

msgid "<32>{#p/napstablook}* oh..."
msgstr "<32>{#p/napstabook}* О..."

msgid "<32>* nevermind..."
msgstr "<32>* Ничего страшного."

msgid "<32>{#p/napstablook}* oh..........."
msgstr "<32>{#p/napstabook}* О..."

msgid ""
"<32>{#p/napstablook}* after a great meal i like to lie on the ground and "
