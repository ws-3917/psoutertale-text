msgstr "<25>{#p/asriel1}{#f/26}{#i/4}* $(name)..."

msgid "<32>{#p/basic}* Shh..."
msgstr "<32>{#p/basic}* 噓..."

msgid "<32>* It's alright."
msgstr ""

msgid "<32>* I've got you, okay?"
msgstr "<32>* 我懂你了，好嗎？"

msgid "<25>{#p/asriel1}{#f/25}{#i/4}* I..."
msgstr "<25>{#p/asriel1}{#f/25}{#i/4}* 我..."

