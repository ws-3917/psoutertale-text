msgstr "<33>{#p/mettaton}* \"СЛЕДИТЕ ЗА НОВОЙ ПРОГРАММОЙ!\""

msgid "<33>{#p/mettaton}* \"NETWORK UNREACHABLE.\""
msgstr "<33>{#p/mettaton}* \"СЕТЬ НЕДОСТУПНА.\""

msgid ""
"<33>{#p/mettaton}* \"SORRY, FOLKS!\"\n"
"* \"THE PROGRAM'S BEEN CANCELLED!\""
msgstr ""
"<33>{#p/mettaton}* \"ИЗВИНИТЕ, РЕБЯТА!\"\n"
"* \"ПРОГРАММА ОТМЕНЕНА!\""

msgid "<32>{#p/mettaton}* \"HOPE YOU ENJOYED THE SHOW!\""
msgstr "<32>{#p/mettaton}* \"НАДЕЮСЬ, ВАМ ПОНРАВИЛОСЬ ШОУ!\""

msgid ""
"<18>{#p/papyrus}{#f/7}WHAT!!!\n"
"IT'S USUALLY BETTER THAN THIS!"
msgstr ""
"<18>{#p/papyrus}{#f/7}ЧТО!!!\n"
"ОБЫЧНО ЭТО ЛУЧШЕ, ЧЕМ ЭТО!"

msgid "<18>{#f/4}THIS IS JUST A BAD EPISODE."
msgstr "<18>{#f/4}ЭТО ПРОСТО ПЛОХОЙ ЭПИЗОД."

msgid "<18>{#f/7}DON'T JUDGE ME!!!"
msgstr "<18>{#f/7}НЕ ОСУЖДАЙТЕ МЕНЯ!!!"

msgid "<18>{#p/papyrus}SO, AS I WAS SAYING ABOUT UNDYNE..."
msgstr "<18>{#p/papyrus}ИТАК, КАК Я УЖЕ ГОВОРИЛ ОБ АНДАЙН..."

msgid ""
"<18>{#p/papyrus}SANS!!\n"
"OH MY GOD!!\n"
"IS THAT..."
msgstr ""
"<18>{#p/papyrus}САНС!!\n"
"О БОЖЕ!!!\n"
"ЭТО..."

msgid "<18>A HUMAN!?!?"
msgstr "<18>ЧЕЛОВЕК!?!?"

msgid "<25>{#p/sans}{#f/2}* nah, it's just a human shaped hologram."
msgstr "<25>{#p/sans}{#f/2}* нет, это просто голограмма в форме человека."

msgid "<18>{#p/papyrus}{#f/4}OH."
msgstr "<18>{#p/papyrus}{#f/4}ОХ."

msgid "<18>{#f/4}..."
msgstr "<18>{#f/4}..."

msgid "<18>{#f/4}WAIT..."
msgstr "<18>{#f/4}ПОДОЖДИ..."

msgid "<18>{#f/7}YOU'RE LYING!!"
msgstr "<18>{#f/7}ТЫ ЛЖЁШЬ!!!"

msgid "<25>{#p/sans}{#f/2}* sorry, meant to say \"hologram-shaped human.\""
msgstr ""
"<25>{#p/sans}{#f/2}* прости, я хотел сказать \"голограммообразный человек.\""

msgid "<18>{#p/papyrus}{#f/0}... SANS, WE FINALLY DID IT!"
msgstr "<18>{#p/papyrus}{#f/0}... САНС, МЫ НАКОНЕЦ-ТО СДЕЛАЛИ ЭТО!"

msgid "<18>{#f/9}UNDYNE -HAS- TO LET ME INTO THE ROYAL GUARD NOW!!!"
msgstr ""
"<18>{#f/9}АНДАЙН - ДОЛЖНА - ТЕПЕРЬ ПРИНЯТЬ МЕНЯ В КОРОЛЕВСКУЮ ГВАРДИЮ!!!"

msgid "<18>{#f/6}WE JUST HAVE TO..."
msgstr "<18>{#f/6}МЫ ПРОСТО ОБЯЗАНЫ..."

msgid "<18>{#f/5}TO..."
msgstr "<18>{#f/5}TO..."

msgid "<18>{#f/4}I'M FORGETTING SOMETHING."
msgstr "<18>{#f/4}Я ЧТО-ТО ЗАБЫЛ."

msgid "<25>{#p/sans}{#f/2}* the speech, remember?"
msgstr "<25>{#p/sans}{#f/2}* речь, помнишь?"

msgid ""
"<18>{#p/papyrus}{#f/4}OH, RIGHT.\n"
"...\"AHEM.\""
msgstr ""
"<18>{#p/papyrus}{#f/4}О, ПРАВИЛЬНО.\n"
"...\"КХМ.\""

msgid "<18>{#f/9}HUMAN! YOU MAY THINK YOU'RE SAFE OUT HERE..."
msgstr "<18>{#f/9}ЧЕЛОВЕК! ТЫ МОЖЕШЬ ДУМАТЬ, ЧТО ЗДЕСЬ ТЫ В БЕЗОПАСНОСТИ..."

msgid "<18>{#f/9}BUT I, THE GREAT PAPYRUS, INTEND TO CHANGE THAT!"
msgstr "<18>{#f/9}НО Я, ВЕЛИКИЙ ПАПИРУС, НАМЕРЕН ЭТО ИЗМЕНИТЬ!"

msgid "<18>{#f/4}FIRST, I'LL DAZZLE YOU WITH DR. ALPHYS' PUZZLES..."
msgstr "<18>{#f/4}СНАЧАЛА Я УДИВЛЮ ТЕБЯ ГОЛОВОЛОМКАМИ ДР. АЛЬФИС..."

msgid "<18>{#f/4}AND THEN, WHEN YOU LEAST EXPECT IT..."
msgstr "<18>{#f/4}А ПОТОМ, КОГДА ТЫ МЕНЬШЕ ВСЕГО ЭТОГО ЖДЁШЬ..."

msgid ""
"<19>{#f/9}WHAM!\n"
"CAPTURED!\n"
"OFF TO THE CITADEL!"
msgstr ""
"<19>{#f/9}ОПА!\n"
"ЗАХВАЧЕН!\n"
"В ЦИТАДЕЛЬ!"

msgid "<18>{#f/9}OUR BATTLE WILL BE AS LEGENDARY AS THEY COME!"
msgstr "<18>{#f/9}НАША БИТВА БУДЕТ ЛЕГЕНДАРНОЙ!"

msgid "<18>{#f/9}CONTINUE... ONLY IF YOU DARE!!!"
msgstr "<18>{#f/9}ПРОДОЛЖАЙ... ТОЛЬКО ЕСЛИ ТЫ ОСМЕЛИШЬСЯ!!!"

msgid "<18>{#f/0}NYEH HEH HEH HEH HEH HEH HEH HEH!!!"
msgstr "<18>{#f/0}НЬЕХ ХЕ-ХЕ-ХЕ-ХЕ-ХЕ-ХЕ-ХЕ!!!"

msgid "<25>{#p/sans}* well, that went well."
msgstr "<25>{#p/sans}* ну, всё прошло хорошо."

msgid "<25>* don't sweat it, bud."
msgstr "<25>* Не переживай, приятель."

msgid "<25>{#f/2}* i'll keep an eyesocket out for ya."
msgstr "<25>{#f/2}* Я присмотрю за тобой."

msgid ""
"<18>{#p/papyrus}{#f/5}H-HUMAN?\n"
"IS THAT YOU...?"
msgstr ""
"<18>{#p/papyrus}{#f/5}Ч-ЧЕЛОВЕК?\n"
"ЭТО ТЫ...?"

msgid ""
"<18>{#p/papyrus}{#f/1}OH MY GOD!!!\n"
"IT REALLY IS YOU, ISN'T IT!?!?"
msgstr ""
"<18>{#p/papyrus}{#f/1}О БОЖЕ!!!\n"
"ЭТО ПРАВДА ТЫ, НЕ ТАК ЛИ!?!?"

msgid ""
"<18>{#p/papyrus}{#f/0}I'VE BEEN EXCITED TO MEET YOU SINCE I HEARD ABOUT YOU!"
msgstr ""
"<18>{#p/papyrus}{#f/0}Я С НЕТЕРПЕНИЕМ ЖДАЛ ВСТРЕЧИ С ТОБОЙ С ТЕХ ПОР, КАК "
"УЗНАЛ О ТЕБЕ!"

msgid "<18>{#p/papyrus}{#f/4}... AS FOR WHY I'M ONLY HERE NOW?"
msgstr "<18>{#p/papyrus}{#f/4}... А ПОЧЕМУ Я ЗДЕСЬ ТОЛЬКО СЕЙЧАС?"

msgid "<18>{#p/papyrus}{#f/6}WELL, I HAVE REASON TO BELIEVE..."
msgstr "<18>{#p/papyrus}{#f/6}НУ, У МЕНЯ ЕСТЬ ПРИЧИНЫ ПОЛАГАТЬ..."

msgid ""
"<18>{#p/papyrus}{#f/5}... MY BROTHER HAS BEEN TRYING TO KEEP YOU FROM ME."
msgstr "<18>{#p/papyrus}{#f/5}... МОЙ БРАТ ПЫТАЛСЯ СКРЫТЬ ТЕБЯ ОТ МЕНЯ."

msgid "<18>{#p/papyrus}{#f/7}TYPICAL!!!"
msgstr "<18>{#p/papyrus}{#f/7}ТИПИЧНО!!!"

msgid ""
"<18>{#p/papyrus}{#f/0}BUT OUR LITTLE ALLIANCE CAN BE OUR LITTLE SECRET."
msgstr ""
"<18>{#p/papyrus}{#f/0}НО НАШ МАЛЕНЬКИЙ АЛЬЯНС МОЖЕТ СТАТЬ НАШИМ МАЛЕНЬКИМ "
"СЕКРЕТОМ."

msgid "<18>{#p/papyrus}{#f/9}HE DOESN'T HAVE TO KNOW A -DAMN- THING ABOUT IT!"
msgstr "<18>{#p/papyrus}{#f/9}ОН НЕ ДОЛЖЕН НИЧЕГО ОБ ЭТОМ ЗНАТЬ!"

msgid "<18>{#p/papyrus}{#f/5}GUESS I BETTER GO BEFORE HE FINDS OUT I'M HERE."
msgstr ""
"<18>{#p/papyrus}{#f/5}ДУМАЮ, МНЕ ЛУЧШЕ УЙТИ, ПОКА ОН НЕ УЗНАЛ, ЧТО Я ЗДЕСЬ."

msgid "<18>{|}{#p/papyrus}{#f/9}I'LL CATCH UP WITH YOU SOON, HU- {%}"
msgstr "<18>{|}{#p/papyrus}{#f/9}Я СКОРО ТЕБЯ ДОГОНЮ, ЧЕ- {%}"

msgid "<32>{#p/without}* ... papyrus?"
msgstr "<32>{#p/without}* ... Папирус?"

msgid "<18>{#f/4}YOU MAY HAVE HAD AN EASY TIME BEFORE."
msgstr "<18>{#f/4}ВОЗМОЖНО, РАНЬШЕ ТЕБЕ БЫЛО ЛЕГКО."

msgid "<18>{#f/4}YOU MAY HAVE PASSED MY OTHER CHALLENGES."
msgstr "<18>{#f/4}ТЫ МОГ ПРОЙТИ И ДРУГИЕ МОИ ИСПЫТАНИЯ."

msgid "<18>{#f/9}BUT NOW YOU WILL SURELY MEET YOUR WIT'S END!"
msgstr "<18>{#f/9}НО ТЕПЕРЬ ТЫ ТОЧНО ДОЙДЁШЬ ДО КОНЦА!"

msgid "<18>FOR YOU SEE, THIS PUZZLE WAS MADE BY NONE OTHER..."
msgstr "<18>ВЕДЬ ЭТУ ГОЛОВОЛОМКУ СОЗДАЛ НЕ КТО ИНОЙ, КАК..."

msgid "<18>{#f/0}THAN THE AMAZING DR. ALPHYS!"
msgstr "<18>{#f/0}ЧЕМ УДИВИТЕЛЬНАЯ ДР. АЛЬФИС!"

msgid "<18>THE RULES ARE QUITE SIMPLE, REALLY."
msgstr "<18>ПРАВИЛА ОЧЕНЬ ПРОСТЫ."

msgid "<18>THIS DISPLAY WILL READ OUT A NUMBER AT RANDOM."
msgstr "<18>НА ДИСПЛЕЕ ПОЯВИТСЯ СЛУЧАЙНЫЙ НОМЕР."

msgid "<18>{#f/9}... THE NUMBER OF SECONDS UNTIL YOU CAN PASS!"
msgstr "<18>{#f/9}... КОЛИЧЕСТВО СЕКУНД, ЗА КОТОРОЕ МОЖНО ПРОЙТИ!"

msgid "<18>{#f/0}IF THE NUMBER IS ODD, YOU MUST DODGE PROJECTILES."
msgstr "<18>{#f/0}ЕСЛИ НОМЕР НЕЧЁТНЫЙ, ТЫ ДОЛЖЕН УКЛОНЯТЬСЯ ОТ СНАРЯДОВ."

msgid "<18>NUMBERS ENDING IN 1 GIVE YOU STAR- SHAPED ONES..."
msgstr "<18>НОМЕРА, ОКАНЧИВАЮЩИЕСЯ НА 1, ДАЮТ ЗВЁЗДНЫЕ ОЧКИ..."

msgid "<18>NUMBERS ENDING IN 3 GIVE YOU MOON- SHAPED ONES..."
msgstr "<18>НОМЕРА, ОКАНЧИВАЮЩИЕСЯ НА 3, ДАЮТ ЛУНООБРАЗНЫЕ..."

msgid "<18>{#f/4}5 GIVES YOU COMETS, 7 GIVES YOU QUASARS..."
msgstr "<18>{#f/4}5 ДАЁТ КОМЕТЫ, 7 ДАЁТ КВАЗАРЫ..."

msgid "<18>{#f/9}AND IF IT ENDS IN A 9, IT'S ENTIRELY RANDOM!"
msgstr "<18>{#f/9}И ЕСЛИ ОНА ЗАКАНЧИВАЕТСЯ НА 9, ТО ЭТО СОВЕРШЕННО СЛУЧАЙНО!"

msgid "<18>{#f/0}IF THE NUMBER IS PRIME, THE GRAVITY WILL FLIP."
msgstr "<18>{#f/0}ЕСЛИ НОМЕР ПРОСТОЙ, ТО ГРАВИТАЦИЯ ПЕРЕВЕРНЁТСЯ."

msgid "<18>{#f/4}(ALTHOUGH, PRIMES BELOW TEN DON'T OFTEN TRIGGER IT.)"
msgstr "<18>{#f/4}(ХОТЯ, ПРАЙМЫ НИЖЕ 10 НЕ ЧАСТО СРАБАТЫВАЮТ)."

msgid "<18>{#f/0}IF THE NUMBER IS EVEN, YOU WILL BE FINE AT FIRST..."
msgstr "<18>{#f/0}ЕСЛИ ЧИСЛО ЧЁТНОЕ, ТО ПОНАЧАЛУ ТЫ БУДЕШЬ В ПОРЯДКЕ..."

msgid "<18>{#f/9}BUT RANDOM MONSTER ENCOUNTERS WILL OCCUR!"
msgstr "<18>{#f/9}НО СЛУЧАЙНЫЕ ВСТРЕЧИ С МОНСТРАМИ БУДУТ ПРОИСХОДИТЬ!"

msgid "<18>ALSO, POWERS OF TWO WILL DOUBLE THE FREQUENCY!!"
msgstr "<18>КРОМЕ ТОГО, СИЛА ДВУХ УДВАИВАЕТ ЧАСТОТУ!!!"

msgid "<18>{#f/0}IF THE NUMBER REPEATS THE SAME DIGIT TWICE..."
msgstr "<18>{#f/0}ЕСЛИ В НОМЕРЕ ДВАЖДЫ ПОВТОРЯЕТСЯ ОДНА И ТА ЖЕ ЦИФРА..."

msgid "<18>{#f/0}THE WAIT TIME WILL BE MULTIPLIED BY SAID DIGIT!"
msgstr "<18>{#f/0}ВРЕМЯ ОЖИДАНИЯ БУДЕТ УМНОЖЕНО НА УКАЗАННУЮ ЦИФРУ!"

msgid "<18>{#f/0}IF THE NUMBER IS A RUN, I.E. 1-2-3..."
msgstr "<18>{#f/0}ЕСЛИ ЧИСЛО ЯВЛЯЕТСЯ ПРОБЕГОМ, ТО ЕСТЬ 1-2-3..."

msgid "<18>{#f/0}THE ROOM WILL SHAKE, CAUSING YOU TO STUMBLE!"
msgstr "<18>{#f/0}КОМНАТА ЗАТРЯСЁТСЯ, И ТЫ СПОТКНЁШЬСЯ!"

msgid "<18>{#f/0}AND IF THE NUMBER CONTAINS A 4 AT ALL..."
msgstr "<18>{#f/0}И ЕСЛИ НОМЕР СОДЕРЖИТ 4..."

msgid "<18>{#f/9}SANS WILL RANDOMLY LEVITATE YOU WITH BLUE MAGIC!"
msgstr ""
"<18>{#f/9}САНС БУДЕТ СЛУЧАЙНО ЛЕВИТИРОВАТЬ ТЕБЯ С ПОМОЩЬЮ СИНЕЙ МАГИИ!"

msgid "<25>{#p/sans}{#f/6}* check it out, it's my special yellow eye."
msgstr "<25>{#p/sans}{#f/6}* Посмотри, это мой особый жёлтый глаз."

msgid "<18>{#p/papyrus}{#f/7}NOT NOW, SANS!!"
msgstr "<18>{#p/papyrus}{#f/7}НЕ СЕЙЧАС, САНС!!"

msgid ""
"<25>{#p/sans}* oh, heheh.\n"
"* guess i got a little {@fill=#ff0}carried away{@fill=#fff}, huh?"
msgstr ""
"<25>{#p/sans}* о хехех.\n"
"* кажется, я немного {@fill=#ff0}увлечённый{@fill=#fff}, да?"

msgid "<18>{#p/papyrus}{#f/4}YEAH, YEAH..."
msgstr "<18>{#p/papyrus}{#f/4}ДА, ДА..."

msgid ""
"<18>{#f/9}WELL!\n"
"DO YOU UNDERSTAND THE EXPLANATION?"
msgstr ""
"<18>{#f/9}ХОРОШО!\n"
"ТЫ ПОНЯЛ ОБЪЯСНЕНИЕ?"

msgid "<18>{#p/papyrus}{#f/9}WELL, LET'S REVIEW THEN!"
msgstr "<18>{#p/papyrus}{#f/9}НУ ЧТО Ж, ТОГДА ДАВАЙ ПОСМОТРИМ!"

msgid "<18>{#f/0}THIS DISPLAY GENERATES A RANDOM NUMBER OF SECONDS."
msgstr "<18>{#f/0}ЭТОТ ДИСПЛЕЙ ГЕНЕРИРУЕТ СЛУЧАЙНЫЕ НОМЕРА В СЕКУНДЫ."

msgid "<18>AKA, HOW LONG YOU MUST WAIT TO PASS THROUGH."
msgstr "<18>ОН ЖЕ - СКОЛЬКО НУЖНО ЖДАТЬ, ЧТОБЫ ПРОЙТИ."

msgid "<18>ODD NUMBERS MEAN DODGING PROJECTILES."
msgstr "<18>НЕЧЁТНЫЕ ЧИСЛА ОЗНАЧАЮТ УКЛОНЕНИЕ ОТ СНАРЯДОВ."

msgid "<18>THE NUMBER'S LAST DIGIT DETERMINES THE TYPE."
msgstr "<18>ПОСЛЕДНЯЯ ЦИФРА НОМЕРА ОПРЕДЕЛЯЕТ ЕГО ТИП."

msgid "<18>1 FOR STARS, 3 FOR MOONS, 5 FOR QUASARS, 7 FOR..."
msgstr "<18> 1 ДЛЯ ЗВЁЗД, 3 ДЛЯ ЛУН, 5 ДЛЯ КВАЗАРОВ, 7 ДЛЯ..."

msgid "<18>{#f/5}WAIT, WHICH NUMBER WAS FOR QUASARS AGAIN??"
msgstr "<18>{#f/5}ПОДОЖДИ, КАКОЙ НОМЕР БЫЛ ДЛЯ КВАЗАРОВ ОПЯТЬ??"

msgid "<18>{#f/9}UH, WELL, IF IT ENDS IN 9, THE TYPE IS RANDOM!"
msgstr "<18>{#f/9}НУ, ЕСЛИ ОН ЗАКАНЧИВАЕТСЯ НА 9, ТО ТИП СЛУЧАЙНЫЙ!"

msgid "<18>{#f/0}PRIME NUMBERS SHIFT THE GRAVITY AROUND..."
msgstr "<18>{#f/0}ПРОСТЫЕ ЧИСЛА СМЕЩАЮТ ГРАВИТАЦИЮ ВОКРУГ..."

msgid "<18>{#f/0}EVEN NUMBERS TRIGGER RANDOM ENCOUNTERS..."
msgstr "<18>{#f/0}ЧЁТНЫЕ ЧИСЛА ВЫЗЫВАЮТ СЛУЧАЙНЫЕ СТОЛКНОВЕНИЯ..."

msgid "<18>{#f/5}WAIT, DID I SAY THE GRAVITY SHIFTS!?,"
msgstr "<18>{#f/5}ПОДОЖДИ, Я СКАЗАЛ, ЧТО ГРАВИТАЦИЯ СМЕЩАЕТСЯ!?"

msgid "<18>{#f/7}UGH, I MEANT TO SAY IT FLIPS!!"
msgstr "<18>{#f/7}УГХ, Я ХОТЕЛ СКАЗАТЬ, ЧТО ОН ПЕРЕВОРАЧИВАЕТСЯ!!!"

msgid "<18>{#f/0}BUT POWERS OF TWO DOUBLE THE ENCOUNTER RATE."
msgstr "<18>{#f/0}НО В ДВА РАЗА БОЛЬШЕ."

msgid "<18>RUNS CAUSE THE ROOM TO SHAKE, AND 4 MEANS..."
msgstr "<18>ОТ БЕГА КОМНАТА ТРЯСЁТСЯ, А 4 ОЗНАЧАЕТ..."

msgid "<18>{#f/6}UH, I THINK I FORGOT WHAT 4 MEANS."
msgstr "<18>{#f/6}КАЖЕТСЯ, Я ЗАБЫЛ, ЧТО ЗНАЧИТ 4."

msgid "<25>{#p/sans}* wasn't that supposed to be my cue?"
msgstr "<25>{#p/sans}* разве это не должно было быть моей репликой?"

msgid "<18>{#p/papyrus}{#f/6}MAYBE???"
msgstr "<18>{#p/papyrus}{#f/6}МОЖЕТ БЫТЬ???"

msgid ""
"<18>{#f/7}WHATEVER!!\n"
"DO YOU UNDERSTAND IT NOW!?"
msgstr ""
"<18>{#f/7}БЕЗ РАЗНИЦЫ!!!\n"
"ТЫ ПОНИМАЕШЬ ЭТО СЕЙЧАС!?"

msgid "Sure"
msgstr "Конечно"

msgid "Even less"
msgstr "Ещё меньше"

msgid "<18>{#p/papyrus}{#f/9}WELL... THEN..."
msgstr "<18>{#p/papyrus}{#f/9}ХОРОШО... ТОГДА..."

msgid "<18>{#f/9}YOU KNOW WHAT, I'LL LEAVE THE INSTRUCTIONS HERE!"
msgstr "<18>{#f/9}ТЫ ЗНАЕШЬ ЧТО, Я ОСТАВЛЮ ИНСТРУКЦИЮ ЗДЕСЬ!"

msgid "<18>{#f/0}THEN, YOU CAN READ THEM AT YOUR OWN PACE."
msgstr "<18>{#f/0}ТОГДА ТЫ СМОЖЕШЬ ЧИТАТЬ ИХ В СВОЁМ СОБСТВЕННОМ ТЕМПЕ."

msgid "<18>GOOD LUCK, HUMAN!!"
msgstr "<18>ХОРОШО УДАЧИ, ЧЕЛОВЕК!!"

msgid "<18>{#f/5}NYEH... HEH HEH..."
msgstr "<18>{#f/5}НЬЕХ... ХЕ-ХЕ..."

msgid "<18>{#p/papyrus}{#f/9}AWESOME SAUCE!"
msgstr "<18>{#p/papyrus}{#f/9}ПОТРЯСАЮЩИЙ СОУС!"

msgid "<18>{#f/9}WELL, WITHOUT FURTHER ADO..."
msgstr "<18>{#f/9}НУ ЧТО Ж, БЕЗ ЛИШНИХ СЛОВ..."

msgid "<18>LET'S FIND OUT WHAT OUR RANDOM NUMBER WILL BE!!"
msgstr "<18>ДАВАЙТЕ УЗНАЕМ, КАКИМ БУДЕТ ТВОЙ СЛУЧАЙНЫЙ НОМЕР!!!"

msgid "<18>{#p/papyrus}{#f/9}HUMAN!"
msgstr "<18>{#p/papyrus}{#f/9}ЧЕЛОВЕК!"

msgid "<18>{#f/9}ARE YOU READY FOR YOUR GREATEST CHALLENGE YET?"
msgstr "<18>{#f/9}ТЫ ГОТОВ К САМОМУ СЛОЖНОМУ ИСПЫТАНИЮ?"

msgid "<18>{#f/9}INTRODUCING... THE GAUNTLET OF DEADLY TERROR!"
msgstr "<18>{#f/9}ПРЕДСТАВЛЯЮ... ПЕРЧАТКА СМЕРТЕЛЬНОГО ТЕРРОРА!"

msgid "<18>{#p/papyrus}{#f/9}ONCE I GIVE THE WORD, IT WILL FULLY ACTIVATE!"
msgstr ""
"<18>{#p/papyrus}{#f/9}КАК ТОЛЬКО Я ДАМ СЛОВО, ОНА ПОЛНОСТЬЮ АКТИВИРУЕТСЯ!"

msgid ""
"<18>LASERS WILL FIRE!\n"
"COILS WILL CHARGE!\n"
"BLADES WILL SLICE!"
msgstr ""
"<18>ЛАЗЕРЫ БУДУТ СТРЕЛЯТЬ!\n"
"КАТУШКИ БУДУТ ЗАРЯЖАТЬСЯ!\n"
"ЛЕЗВИЯ БУДУТ РЕЗАТЬ!"

msgid "<18>ALL IN A PRECISE AND TACTICAL MANNER!"
