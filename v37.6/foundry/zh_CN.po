msgstr "<10>{#p/undyne}{#f/1}* 哦，抱歉，我讲太多了..."

msgid "<10>{#p/undyne}{#f/12}* Oh, sorry, I was talking for so long..."
msgstr "<10>{#p/undyne}{#f/12}* 哦，抱歉，我讲太多了..."

msgid "<10>{#f/12}* You're out of water, aren't you?"
msgstr "<10>{#f/12}* 你的水喝完了，是吧？"

msgid "<10>{#f/12}* You're out of punch, aren't you?"
msgstr "<10>{#f/12}* 你的潘趣酒喝完了，是吧？"

msgid "<10>{#f/12}* You're out of cocoa, aren't you?"
msgstr ""
"<10>{#f/12}* 你的热巧克力喝完了，\n"
