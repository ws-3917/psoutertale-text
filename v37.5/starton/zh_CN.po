msgstr ">{#p/sans}* 你这不是在暗示我吗？"

msgid ">{#p/papyrus}{#f/6}MAYBE???"
msgstr ">{#p/papyrus}{#f/6}可能吧？？？"

msgid ""
">{#f/7}WHATEVER!!\n"
"DO YOU UNDERSTAND IT NOW!?"
msgstr ""
">{#f/7}管他呢！！\n"
"你现在理解了吗！？"

