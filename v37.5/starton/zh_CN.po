msgstr ">{#p/sans}* 可能在研究路线吧。"

msgid ">{#p/papyrus}{#f/4}(OH, TRUE.)"
msgstr ">{#p/papyrus}{#f/4}（哦，确实。）"

msgid ">{#f/9}IN THAT CASE, PROCEED WHEN READY!"
msgstr ""
">{#f/9}那样的话，\n"
"准备好了就前进吧！"

msgid ""
">{#p/sans}{#f/0}* whoops.\n"
"* nice try, though."
msgstr ""
">{#p/sans}{#f/0}* 哎呀，真可惜。\n"
"* 不过别灰心。"

