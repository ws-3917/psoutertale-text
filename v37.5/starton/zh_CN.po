msgstr ">{#p/sans}{#f/2}* 哦，顺带再复习下{@fill=#00a2e8}蓝色攻击{@fill=#fff}。"

msgid ">{#p/sans}{#f/3}* oh well."
msgstr ">{#p/sans}{#f/3}* 就是这样。"

msgid ">{#p/sans}{#f/0}* am i wrong?"
msgstr ">{#p/sans}{#f/0}* 我说错了？"

msgid ">{#p/sans}{#f/0}* all sorts of attacks."
msgstr ">{#p/sans}{#f/0}* 还有其他攻击。"

