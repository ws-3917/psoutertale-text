msgstr "<32>{#p/basic}* Отключен."

msgid "<25>{#p/asriel2}{#f/8}* Guess there's only one way forward now."
msgstr "<25>{#p/asriel2}{#f/8}* Кажется, что теперь есть только один путь."

msgid "<32>{#p/human}* (The elevator appears to be powered down.)"
msgstr "<32>{#p/human}* (лифт, как представляется, отключен.)"

msgid "<25>{#p/asriel2}{#f/8}* No elevator for us."
msgstr "<25>{#p/asriel2}{#f/8}* У нас нет лифта."

msgid "<25>{#p/asriel2}{#f/8}* Guess we'll have to find another way up."
msgstr ""
"<25>{#p/asriel2}{#f/8}* Похоже, что мы должны найти другой путь к подъему."

