msgstr "<25>{#g/alphysNeutralSweat}* Есть еще два."

msgid "<25>{#p/alphys}{#g/alphysWelp}* One left."
msgstr "<25>{#p/alphys}{#g/alphysWelp}* Слева."

msgid ""
"<25>{#p/alphys}{#g/alphysHellYeah}* Yes!!\n"
"* That's the last one!!"
msgstr "* Это последний!"

msgid "<25>{#g/alphysCutscene2}* Eheh..."
msgstr "<25>{#g/alphysCutscene2}* Э..."

msgid "<25>{#f/10}* ..."
msgstr "<25>{#f/10}* ..."

msgid "<25>{#f/3}* Don't judge me, I just like rooting for you."
msgstr "<25>{#f/3}* Не суди меня, я просто люблю подбадривать тебя."

