msgstr "<18>{#f/4}無論如何..."

msgid "<18>{#f/0}I AM EXCITED FOR WHAT COMES NEXT!"
msgstr ""
"<18>{#f/0}我很期待\n"
"接下來的發展！"

msgid "<18>{#f/4}A PUZZLE SO CONFOUNDING..."
msgstr ""
"<18>{#f/4}一個令人\n"
"十分困惑的謎題..."

