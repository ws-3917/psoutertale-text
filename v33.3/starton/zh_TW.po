msgid "<15>{#p/papyrus}{#f/14}WAIT... I THINK I GET IT NOW!"
msgstr "<15>{#p/papyrus}{#f/14}等等... 我明白了！"

msgid "<15>{#p/papyrus}{#f/15}THE PRINCE IS JEALOUS OF YOUR AFFECTION FOR ME!"
